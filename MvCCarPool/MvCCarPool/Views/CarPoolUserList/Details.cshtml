@model IEnumerable<MvCCarPool.Models.CarPoolUserList>

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CarPoolList.Destination)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CarPoolUser.FName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CarPoolList.Destination)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CarPoolUser.FName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Idx }) |
            @Html.ActionLink("Details", "Details", new { id=item.Idx }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Idx })
        </td>
    </tr>
}

</table>
